{
  "description": "Surf the Merveilles webring from your browser toolbar!",
  "manifest_version": 2,
  "name": "webring",
  "version": "1.3.5",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
  	"browser_style": true,
    "default_icon": "icons/icon-black.svg",
    "theme_icons": [{
    	"light": "icons/icon-white.svg",
    	"dark": "icons/icon-black.svg",
    	"size": 32
    }]
  }
}
